{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "src",
  "projects": {
    "lib": {
      "root": "src/lib",
      "sourceRoot": "src/lib/src",
      "projectType": "library",
      "prefix": "fs",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "src/lib/tsconfig.lib.json",
            "project": "src/lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "src/lib/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/lib/src/test.ts",
            "tsConfig": "src/lib/tsconfig.spec.json",
            "karmaConfig": "src/lib/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/lib/tsconfig.lib.json",
              "src/lib/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "examples": {
      "root": "src/examples/",
      "sourceRoot": "src/examples/src",
      "projectType": "application",
      "prefix": "ex",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss",
          "spec": false
        },
        "@schematics/angular:class": {
          "spec": false
        },
        "@schematics/angular:directive": {
          "spec": false
        },
        "@schematics/angular:guard": {
          "spec": false
        },
        "@schematics/angular:module": {
          "spec": false
        },
        "@schematics/angular:pipe": {
          "spec": false
        },
        "@schematics/angular:service": {
          "spec": false
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/examples",
            "index": "src/examples/src/index.html",
            "main": "src/examples/src/main.ts",
            "polyfills": "src/examples/src/polyfills.ts",
            "tsConfig": "src/examples/tsconfig.app.json",
            "assets": [
              "src/examples/src/favicon.ico",
              "src/examples/src/assets"
            ],
            "styles": [
              "src/examples/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/examples/src/environments/environment.ts",
                  "with": "src/examples/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "examples:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "examples:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "examples:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/examples/src/test.ts",
            "polyfills": "src/examples/src/polyfills.ts",
            "tsConfig": "src/examples/tsconfig.spec.json",
            "karmaConfig": "src/examples/karma.conf.js",
            "styles": [
              "src/examples/src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "src/examples/src/favicon.ico",
              "src/examples/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/examples/tsconfig.app.json",
              "src/examples/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "e2e": {
      "root": "src/e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "src/e2e/protractor.conf.js",
            "devServerTarget": "examples:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "examples:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "src/e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "lib",
  "schematics": {
    "@schematics/angular:class": {
      "spec": false
    },
    "@schematics/angular:component": {
      "spec": false,
      "prefix": "example",
      "styleext": "scss"
    },
    "@schematics/angular:directive": {
      "spec": false,
      "prefix": "example"
    },
    "@schematics/angular:guard": {
      "spec": false
    },
    "@schematics/angular:module": {
      "spec": false
    },
    "@schematics/angular:pipe": {
      "spec": false
    },
    "@schematics/angular:service": {
      "spec": false
    }
  },
  "cli": {
    "packageManager": "yarn"
  }
}
