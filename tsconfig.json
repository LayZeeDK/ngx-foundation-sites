{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "dom",
      "dom.iterable",
      "es2017"
    ],
    "moduleResolution": "node",
    "outDir": "./dist/out-tsc",
    "paths": {
      "ngx-foundation-sites": [
        "dist/lib",
        "src/lib/src"
      ],
      "ngx-foundation-sites/*": [
        "dist/lib/*",
        "src/lib/src/*"
      ]
    },
    "sourceMap": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ]
  }
}