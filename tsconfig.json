{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "declaration": false,
    "downlevelIteration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "lib": ["dom", "dom.iterable", "es2017"],
    "module": "esnext",
    "moduleResolution": "node",
    "outDir": "./dist/out-tsc",
    "paths": {
      "ngx-foundation-sites": ["dist/lib", "src/lib/src"],
      "ngx-foundation-sites/*": ["dist/lib/*", "src/lib/src/*"]
    },
    "sourceMap": true,
    "target": "es2015",
    "typeRoots": ["node_modules/@types"]
  }
}
