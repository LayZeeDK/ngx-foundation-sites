{
  "private": true,
  "scripts": {
    "assets": "run-p assets:*",
    "assets:readme": "copyfiles README.md dist/lib",
    "build": "ng build --prod",
    "postbuild": "npm run assets",
    "build:app": "ng build app --prod",
    "clean": "rimraf dist",
    "e2e": "ng e2e",
    "e2e:watch": "npm run watch -- e2e",
    "lint": "run-s lint:lib lint:app lint:e2e",
    "lint:app": "ng lint app",
    "lint:e2e": "ng lint e2e",
    "lint:lib": "ng lint lib",
    "lint:watch": "npm run watch -- lint",
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "watch": "npm-watch"
  },
  "watch": {
    "e2e": {
      "patterns": [
        "src/e2e/src",
        "src/app/src"
      ],
      "extensions": [
        "html",
        "scss",
        "ts"
      ]
    },
    "lint": {
      "patterns": [
        "src/lib/src",
        "src/app/src",
        "src/e2e/src"
      ],
      "extensions": [
        "ts"
      ]
    },
    "lint:app": {
      "patterns": [
        "src/app/src"
      ],
      "extensions": [
        "ts"
      ]
    },
    "lint:e2e": {
      "patterns": [
        "src/e2e/src"
      ],
      "extensions": [
        "ts"
      ]
    },
    "lint:lib": {
      "patterns": [
        "src/lib/src"
      ],
      "extensions": [
        "ts"
      ]
    }
  },
  "dependencies": {
    "@angular/animations": "~9.0.0-rc.7",
    "@angular/cdk": "^9.0.0-rc.6",
    "@angular/common": "~9.0.0-rc.7",
    "@angular/compiler": "~9.0.0-rc.7",
    "@angular/core": "~9.0.0-rc.7",
    "@angular/platform-browser": "~9.0.0-rc.7",
    "@angular/platform-browser-dynamic": "~9.0.0-rc.7",
    "@angular/router": "~9.0.0-rc.7",
    "foundation-sites": "^6.6.1",
    "rxjs": "^6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.0-rc.7",
    "@angular-devkit/build-ng-packagr": "~0.900.0-rc.7",
    "@angular/cli": "~9.0.0-rc.7",
    "@angular/compiler-cli": "~9.0.0-rc.7",
    "@angular/language-service": "~9.0.0-rc.7",
    "@ngneat/spectator": "^4.10.1",
    "@types/jasmine": "^3.3.8",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^12.11.1",
    "angular2-template-loader": "^0.6.2",
    "codelyzer": "^5.1.2",
    "copyfiles": "^2.1.0",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.1",
    "ng-packagr": "^9.0.0-rc.3",
    "npm-run-all": "^4.1.3",
    "npm-watch": "^0.3.0",
    "protractor": "^5.4.1",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.15.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "~3.6.4",
    "wallaby-webpack": "^3.9.12"
  }
}
