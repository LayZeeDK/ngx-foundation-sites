{
  "private": true,
  "scripts": {
    "assets": "run-p assets:*",
    "assets:readme": "copyfiles README.md dist/lib",
    "build": "ng build --prod",
    "postbuild": "npm run assets",
    "build:app": "ng build app --prod",
    "clean": "rimraf dist",
    "e2e": "ng e2e",
    "e2e:watch": "npm run watch -- e2e",
    "lint": "run-s lint:lib lint:app lint:e2e",
    "lint:app": "ng lint app",
    "lint:e2e": "ng lint e2e",
    "lint:lib": "ng lint lib",
    "lint:watch": "npm run watch -- lint",
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "watch": "npm-watch"
  },
  "watch": {
    "e2e": {
      "patterns": [
        "src/e2e/src",
        "src/app/src"
      ],
      "extensions": [
        "html",
        "scss",
        "ts"
      ]
    },
    "lint": {
      "patterns": [
        "src/lib/src",
        "src/app/src",
        "src/e2e/src"
      ],
      "extensions": [
        "ts"
      ]
    },
    "lint:app": {
      "patterns": [
        "src/app/src"
      ],
      "extensions": [
        "ts"
      ]
    },
    "lint:e2e": {
      "patterns": [
        "src/e2e/src"
      ],
      "extensions": [
        "ts"
      ]
    },
    "lint:lib": {
      "patterns": [
        "src/lib/src"
      ],
      "extensions": [
        "ts"
      ]
    }
  },
  "dependencies": {
    "@angular/common": "~6.0.9",
    "@angular/compiler": "~6.1.9",
    "@angular/core": "~6.0.9",
    "@angular/platform-browser": "~6.0.9",
    "@angular/platform-browser-dynamic": "~6.0.9",
    "@angular/router": "~6.0.9",
    "core-js": "^2.5.7",
    "foundation-sites": "^6.5.0-rc.3",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.4",
    "@angular-devkit/build-ng-packagr": "~0.8.4",
    "@angular/cli": "~6.2.4",
    "@angular/compiler-cli": "~6.1.9",
    "@angular/language-service": "~6.0.9",
    "@types/jasmine": "^2.8.8",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^8.10.32",
    "angular2-template-loader": "^0.6.2",
    "codelyzer": "^4.4.4",
    "copyfiles": "^2.1.0",
    "jasmine-core": "^3.2.1",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.3.1",
    "ng-packagr": "^4.2.0",
    "npm-run-all": "^4.1.3",
    "npm-watch": "^0.3.0",
    "protractor": "^5.4.1",
    "puppeteer": "^1.8.0",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.1",
    "tsickle": ">=0.33.0",
    "tslib": "^1.9.3",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "~2.9.2",
    "wallaby-webpack": "^3.9.12"
  }
}